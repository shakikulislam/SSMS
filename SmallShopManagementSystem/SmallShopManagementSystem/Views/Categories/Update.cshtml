@model SSMS.Models.Models.Category


@{
    ViewBag.Title = "Update";
    Layout = "~/Views/Shared/_Layout.cshtml";

}


<form method="POST">
    <h1>Category Update</h1>
    @if (ViewBag.SUpdate != null)
    {
        <p>@ViewBag.SUpdate</p>
    }
    else if (ViewBag.FUpdate != null)
    {
        <p>@ViewBag.FUpdate</p>
    }
    else if (ViewBag.FMsg != null)
    {
        <p>@ViewBag.FMsg</p>
    }
    <table>
        <tr><td>@Html.LabelFor(c => c.Code)</td></tr>
        <tr><td>@Html.TextBoxFor(c => c.Code)</td></tr>
        <tr><td>@Html.LabelFor(c => c.Name)</td></tr>
        <tr><td>@Html.TextBoxFor(c => c.Name)</td></tr>


        <tr><td><input type="submit" value="Update" /></td></tr>

    </table>
</form>